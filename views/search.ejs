<%- include('./layout/main_layouts.ejs') -%>
    <table class="table">
        <thead>
            <tr>
                <th scope="col-4">First</th>
                <th scope="col-4">Last</th>
                <th scope="col-4">Handle</th>
                <th scope="col-4">name</th>
            </tr>
        </thead>
        <tbody>
            <% if(search !='' ) { %>
                <% search.forEach(function(search){ %>
                    <tr>

                        <td>
                            <%= search.email %>
                        </td>
                        <td>
                            <%= search.name %>
                        </td>
                        <td>
                            <%= search.day %>
                        </td>
                        <td>
                            <%= search.time %>
                        </td>
                    </tr>
                    <% }) %>
            <% } else { %>
                <p>No items found.</p>
              <% } %>

    </table>
    </table>