<%- include('./layout/main_layouts.ejs') -%> 
<table class="table">
    <thead>
      <tr>
        <th scope="col-4">Name</th>
        <th scope="col-4">Email</th>
        <th scope="col-4">Phone No</th>
    </tr>
    </thead>
    <tbody>
        <% if(typeof employees !=='undefined' && employees.length> 0) { %>
            <% employees.forEach(function(Attendance){ %>
                <tr>

                <td>
                    <%= Attendance.fname %>
                    <%= Attendance.lname %>
                </td>
                <td>
                    <%= Attendance.email %>
                </td>
                <td>
                    <%= Attendance.txtEmpPhone %>
                </td>
            </tr>
                <% }) %>
        <% } %>
        <td></td>
    
    </table>
  </table>